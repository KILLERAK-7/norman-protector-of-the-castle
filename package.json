{
  "private": true,
  "scripts": {
    "dev": "vite",
    "prebuild": "rm -rf dist dist.zip",
    "build": "tsc && vite build && npm run archive",
    "build:full": "tsc && vite build && npm run squash && npm run archive",
    "squash": "tinypng dist/",
    "archive": "cd dist && zip -9 ../dist.zip -r * && cd ../ && ./scripts/measure.sh dist.zip",
    "typecheck": "tsc",
    "preview": "vite preview",
    "start":"npm run build && node dist/index.html"
  },

  
  "devDependencies": {
    "@types/express": "^4.17.17",
    "terser": "^5.14.2",
    "tinypng-cli": "^0.0.7",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5",
    "vite": "^2.9.9"
  },
  "dependencies": {
    "express": "^4.18.2"
  }
}
